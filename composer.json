{
    "name": "stackflows-com/laravel-plugin-stackflows",
    "description": "Laravel plugin for Stackflows",
    "keywords": [
        "Stackflows",
        "laravel",
        "laravel-plugin-stackflows"
    ],
    "homepage": "https://github.com/stackflows-com/laravel-plugin-stackflows",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey A.",
            "email": "support@stackflows.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37",
        "ext-pcntl": "*",
        "ext-json": "*",
        "endpot/camunda-rest-client": "^1.1.5"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "jetbrains/phpstorm-attributes": "^1.0",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Stackflows\\StackflowsPlugin\\": "src",
            "Stackflows\\StackflowsPlugin\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stackflows\\StackflowsPlugin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stackflows\\StackflowsPlugin\\StackflowsServiceProvider"
            ],
            "aliases": {
                "LaravelPluginStackflows": "Stackflows\\StackflowsPlugin\\StackflowsPluginFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
